{
  "name": "rehab-monorepo",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Adam Turner <adam.turner@and.digital>",
  "license": "MIT",
  "scripts": {
    "git": "git cz",
    "dev": "skaffold dev",
    "activity-recorder:coverage": "cd activity-recorder && yarn coverage",
    "auth:coverage": "cd auth && yarn coverage",
    "mobile:coverage": "cd mobile && yarn coverage",
    "coverage": "yarn auth:coverage && yarn activity-recorder:coverage && yarn mobile:coverage"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "expo": "^37.0.12"
  }
}
